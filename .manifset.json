{
  "name": "Speedo",
  "version": "0.1",
  "description": "An extension that intelligently tunes your playback preference",
  "permissions": [
    "webNavigation", 
    "tabs", 
    "https://us-central1-speedo-249504.cloudfunctions.net/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content_script.js"]
    }
  ],
  "manifest_version": 2
}
